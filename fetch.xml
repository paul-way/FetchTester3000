<fetch version='1.0' output-format='xml-platform' mapping='logical' distinct='true'>
	<entity name='bp_onpointtarget'>
		<attribute name='bp_onpointtargetid' />
		<attribute name='bp_companyid' />
		<attribute name='bp_name' />
		<attribute name='bp_nextsteps' />
		<attribute name='bp_shortdescription' />
		<attribute name='bp_sector' />
		<attribute name='bp_source' />
		<attribute name='bp_trackingcategory' />
		<order attribute='bp_name' descending='false' />
		<filter type='and'>
			<condition attribute='bp_name' operator='like' value='%tow%' />
			<condition attribute='bp_onpointteamid' operator='eq' value='{12145086-8EAC-E011-8E1B-005056B37E8A}' />
		</filter>
		<link-entity name='account' from='accountid' to='bp_companyid' alias='aa' link-type='outer'>
			<link-entity name='bp_financial' from='bp_companyid' to='accountid' alias='financials' link-type='outer' >
				<attribute name='bp_sales' />
				<attribute name='bp_ebitda' />
				<attribute name='bp_periodending' />
				<order attribute='bp_periodending' descending='true' />
			</link-entity>
		</link-entity>
		<link-entity name='bp_bp_onpointtarget_systemuser' from='bp_onpointtargetid' to='bp_onpointtargetid' alias='mm1' link-type='outer'>
			<link-entity name='systemuser' from='systemuserid' to='systemuserid' alias='team' link-type='outer'>
				<attribute name='systemuserid' />
				<attribute name='fullname' />
				<attribute name='bp_initials' />				
				<filter type='and'>
					<condition attribute='isdisabled' operator='eq' value='0' />
					<condition attribute='accessmode' operator='ne' value='3' />
					<condition attribute='bp_departmentid' operator='ne' uiname='Investment Support' uitype='bp_department' value='{EBE1CF94-BD94-DF11-9F10-005056B37E8A}' />
					<condition attribute='bp_initials' operator='not-null' />					
				</filter>				
			</link-entity>
		</link-entity>
	</entity>
</fetch>